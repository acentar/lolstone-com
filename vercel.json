{
  "buildCommand": "npm run vercel-build",
  "outputDirectory": "dist",
  "framework": null,
  "rewrites": [
    {
      "source": "/api/(.*)",
      "destination": "/api/$1"
    },
    {
      "source": "/card/:id",
      "destination": "/card/[id].html"
    },
    {
      "source": "/news/:slug",
      "destination": "/news/[slug].html"
    },
    {
      "source": "/player/game/:id",
      "destination": "/player/game/[id].html"
    },
    {
      "source": "/player/deck/:id",
      "destination": "/player/deck/[id].html"
    }
  ]
}
